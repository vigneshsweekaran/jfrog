management.listener.port  = 5672
management.ssl.port       = 5671
management.ssl.cacertfile = /opt/xray/var/data/rabbitmq/certs/ca-cert.pem
management.ssl.certfile   = /opt/xray/var/data/rabbitmq/certs/server-cert.pem
management.ssl.keyfile    = /opt/xray/var/data/rabbitmq/certs/server-key.pem
management.listener.ssl   = true